{
  "name": "@ai-observability/client",
  "version": "1.0.0",
  "description": "Frontend observability client for tracking user events, UI interactions, and errors",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run test",
    "clean-react": "rm -rf node_modules/react node_modules/react-dom node_modules/.cache",
    "prelink": "npm run clean-react",
    "link": "npm run build && npm link",
    "pack": "npm run build && npm pack"
  },
  "keywords": [
    "observability",
    "tracking",
    "analytics",
    "telemetry",
    "opentelemetry",
    "monitoring"
  ],
  "author": "AI Observability Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-observability.git",
    "directory": "packages/observability-client"
  },
  "bugs": {
    "url": "https://github.com/your-org/ai-observability/issues"
  },
  "homepage": "https://github.com/your-org/ai-observability#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.2.0"
  }
}
